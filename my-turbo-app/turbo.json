{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": []
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "drizzle:push": {
      "cache": false,
      "interactive": false
    },
    "drizzle:studio": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [],
  "globalDependencies": [
    ".env*",
    "tsconfig.json"
  ]
}