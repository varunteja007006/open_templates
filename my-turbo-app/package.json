{
  "name": "my-turbo-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:starter-convex": "turbo dev --filter=starter-convex",
    "dev:starter-app-convex": "turbo dev --filter=starter-app-convex",
    "dev:starter-app": "turbo dev --filter=starter-app",
    "dev:trpc-app": "turbo dev --filter=trpc-app",
    "build:trpc-app": "turbo build --filter=trpc-app",
    "start:trpc-app": "pnpm --filter=trpc-app start"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20"
  }
}