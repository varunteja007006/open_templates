{
  "name": "my-turbo-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:starter-app-trpc": "turbo dev --filter=starter-app-trpc",
    "dev:starter-app": "turbo dev --filter=starter-app",
    "dev:convex-app": "turbo dev --filter=convex-app",
    "dev:starter-convex": "turbo dev --filter=starter-convex",
    "build:starter-app-trpc": "turbo build --filter=starter-app-trpc",
    "build:starter-app": "turbo build --filter=starter-app",
    "build:convex-app": "turbo build --filter=convex-app",
    "build:starter-convex": "turbo build --filter=starter-convex",
    "start:starter-app-trpc": "pnpm --filter=starter-app-trpc start",
    "start:starter-app": "pnpm --filter=starter-app start",
    "start:convex-app": "pnpm --filter=convex-app start",
    "start:starter-convex": "pnpm --filter=starter-convex start"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "dotenv-cli": "^11.0.0"
  }
}